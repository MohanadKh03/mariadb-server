    "authors": [
      {
        "email": "info@mariadb.com",
        "name": "MariaDB"
      }
    ],
    "properties": [
      {
        "name": "package_name",
        "value": "@CPACK_PACKAGE_FILE_NAME@"
      }
    ],
    "timestamp": "@TIMESTAMP@"
  },
  "components": [
   @sbom_components@
  ],
  "dependencies": [
   @sbom_dependencies@
  ]
